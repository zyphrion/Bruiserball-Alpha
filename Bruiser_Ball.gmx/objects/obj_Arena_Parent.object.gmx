            <string>///Initilization
//Wall Creator - This belongs in a different object
//Will need a full screen(buffer_length pixel)buffer around the edges, as not to displace viewport
//The walls will be recessed INTO the buffer
//17600
//This will create the "arena"
buffer_length = 2000;
side_length = 9760;
end_length = 4880;
thickness = 160;
corner_length = 602;
corner_height = 602;
side_length_total = side_length+(corner_length*2);
end_length_total = end_length+(corner_height*2);

center_x = ((side_length+2*corner_length)/2)+buffer_length;
center_y = ((end_length-2*corner_height)/2)+buffer_length;
x = center_x;
y = center_y;

//walls[ID, x, y, sprite]
wall_west[]  = instance_create((x-side_length_total/2)+(thickness/2),y,obj_End_Wall);
wall_east[]  = instance_create((x+side_length_total/2)-(thickness/2),y,obj_End_Wall);
wall_north[] = instance_create(x,(y-end_length_total/2)+(thickness/2),obj_Side_Wall);
wall_south[] = instance_create(x,(y+end_length_total/2)-(thickness/2),obj_Side_Wall);

//corners[ID, x, y, sprite]
corner_nw = instance_create(x-(side_length_total/2),y-(end_length_total/2),obj_Corner_1);
corner_ne = instance_create(x+(side_length_total/2)-corner_length,y-(end_length_total/2),obj_Corner_2);
corner_se = instance_create(x+(side_length_total/2)-corner_height,y+(end_length_total/2)-corner_height,obj_Corner_3);
corner_sw = instance_create(x-(side_length_total/2),y+(end_length_total/2)-corner_height,obj_Corner_4);
</string>
